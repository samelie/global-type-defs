{
  "name": "@adddog/global-type-defs",
  "version": "0.0.2",
  "type": "module",
  "exports": {
    "types": "./index.d.ts",
    "import": "./index.d.ts"
  },
  "types": "./index.d.ts",
  "module": "./index.d.ts",
  "typings": "./index.d.ts",
  "files": [
    "*.d.ts"
  ],
  "dependencies": {
    "@total-typescript/ts-reset": "^0.6.1"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "types": "tsc -p tsconfig.typecheck.json",
    "build": "echo 'No build needed - distributing .d.ts files'",
    "test": "echo '⚠️⚠️⚠️no test vitest --watch=false'",
    "dev": "tsx src/index.ts",
    "knip": "knip",
    "release": "bumpp --yes --no-push --no-tag --release patch",
    "prepublishOnly": "pnpm run lint && pnpm run types && pnpm run build",
    "prepack": "pnpm run prepublishOnly",
    "version:patch": "npm version patch --no-git-tag-version"
  },
  "devDependencies": {
    "@adddog/monorepo-consistency": "workspace:*",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.0",
    "knip": "^5.83.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  },
  "engines": {
    "node": ">= 22",
    "pnpm": ">= 10",
    "npm": ">= 11"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
